dataLayer=[];var campaignUserIdField="CampaignUserId";var timeLimit=180000;function checkCampaignUserId(){var a=getUrlParams(campaignUserIdField);var e=new Date();if(window.sessionStorage){var f=JSON.parse(sessionStorage.getItem(campaignUserIdField));if(a!==""){if(f){var c=f.content;var b=f.timestamp;var d=e.getTime()-b;if((c===a)&&(d>timeLimit)){sessionStorage.removeItem(campaignUserIdField);dataLayer.push({CampaignUserId:""})}else{sessionStorage.setItem(campaignUserIdField,JSON.stringify({content:a,timestamp:new Date().getTime()}));dataLayer.push({CampaignUserId:a})}}else{sessionStorage.setItem(campaignUserIdField,JSON.stringify({content:a,timestamp:new Date().getTime()}));dataLayer.push({CampaignUserId:a})}}else{if(f){var c=f.content;var b=f.timestamp;var d=e.getTime()-b;if(d>timeLimit){sessionStorage.removeItem(campaignUserIdField);dataLayer.push({CampaignUserId:""})}else{sessionStorage.setItem(campaignUserIdField,JSON.stringify({content:c,timestamp:new Date().getTime()}));dataLayer.push({CampaignUserId:c})}}}}}function getUrlParams(a){var c=window.location.search.substring(1);var e=c.split("&");for(var b=0;b<e.length;b++){var d=e[b].split("=");if(d[0]==a){return d[1]}}return""}checkCampaignUserId();